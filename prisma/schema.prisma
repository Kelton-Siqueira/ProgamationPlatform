// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PostUsers {
    id Int @default(autoincrement()) @id
    title String
    body String
}

model User{
    id Int @default(autoincrement()) @id
    id_user Int
    name String
    email String
}
